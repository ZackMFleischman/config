{
    "editor.minimap.enabled": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.stablePeek": false,
    "editor.codeLens": false,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/tmp": true,
        "**/Builds": true,
        "**/Plugins": true,
        "**/StompyRobot": true,
        "**/AssetBundles": true,
        "**/TextMesh Pro": true,
        "**/EasyTouchBundle": true,
        "**/ConsoleE": true,
        "**/Demigiant": true,
        "**/Dynamic Decals": true,
        "**/*.csproj": true,
        "**/Sqlite.cs": true,
        "**/KeenClient.cs": true,
        "**/UnityTestTools": true,
        "**/JsonDotNet": true
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "build/": true,
        "Build/": true,
        "AssetBundles/": true,
        "Library/": true,
        "TPS_cache/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "ProjectSettings/": true,
        "temp/": true,
        "Temp/": true
    },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.autoindent": true,
    "vim.statusBarColorControl": false,
    "vim.statusBarColors": {
        "normal": "#005f5f",
        "insert": "#5C0081",
        "visual": "#007022",
        "visualline": "#007022",
        "visualblock": "#86592d",
        "replace": "#000000"
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.otherModesKeyBindings": [],
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "v"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.splitEditor"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "_"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.navigateForward"
                }
            ]
        },
        {
            "before": [
                "-"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.navigateBack"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "i"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.view.explorer"
                }
            ]
        },
        {
            "before": [
                "]"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.nextEditor"
                }
            ]
        },
        {
            "before": [
                "["
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.previousEditor"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.quickOpen"
                }
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "2",
                "0",
                "k"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "2",
                "0",
                "j"
            ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":nohl"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                },
                {
                    "command": "workbench.action.closeActiveEditor",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "x"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "args": []
                }
            ]
        },
        
        {
            "before": [
                "<leader>",
                "a"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.findInFiles",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "\"",
                "c"
            ],
            "commands": []
        },
        {
            "before": [
                "<leader>",
                "P"
            ],
            "after": [
                "$",
                "p"
            ],
            "commands": []
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    }
}